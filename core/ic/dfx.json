{
    "canisters": {
        "relayer": {
			  "candid": "src/agents/relayer.did",
			  "package": "omnic",
			  "type": "rust",
			  "wasm": "target/wasm32-unknown-unknown/release/omnic.wasm",
			  "build": [
				"cargo build --target wasm32-unknown-unknown --release",
				"ic-cdk-optimizer target/wasm32-unknown-unknown/release/relayer.wasm -o target/wasm32-unknown-unknown/release/relayer_opt.wasm"
			  ]
        },
        "demo": {
            "candid": "src/demo.did",
            "package": "demo",
            "type": "rust",
            "wasm": "target/wasm32-unknown-unknown/release/demo.wasm",
            "build": [
              "cargo build --target wasm32-unknown-unknown --release",
              "ic-cdk-optimizer target/wasm32-unknown-unknown/release/demo.wasm -o target/wasm32-unknown-unknown/release/demo_opt.wasm"
            ]
      }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        }
    },
    "version": 1
}
