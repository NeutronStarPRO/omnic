{
    "canisters": {
        "relayer": {
			  "candid": "src/agents/relayer.did",
			  "type": "custom",
			  "wasm": "target/wasm32-unknown-unknown/release/relayer_opt.wasm",
			  "build": [
				"cargo build --target wasm32-unknown-unknown --bin relayer --release",
				"ic-cdk-optimizer target/wasm32-unknown-unknown/release/relayer.wasm -o target/wasm32-unknown-unknown/release/relayer_opt.wasm"
			  ]
        },
        "proxy": {
            "candid": "src/agents/proxy.did",
            "type": "custom",
            "wasm": "target/wasm32-unknown-unknown/release/proxy_opt.wasm",
            "build": [
              "cargo build --target wasm32-unknown-unknown --bin proxy --release",
              "ic-cdk-optimizer target/wasm32-unknown-unknown/release/proxy.wasm -o target/wasm32-unknown-unknown/release/proxy_opt.wasm"
            ]
        },
        "demo": {
            "candid": "src/agents/demo.did",
            "type": "custom",
            "wasm": "target/wasm32-unknown-unknown/release/demo_opt.wasm",
            "build": [
              "cargo build --target wasm32-unknown-unknown --bin demo --release",
              "ic-cdk-optimizer target/wasm32-unknown-unknown/release/demo.wasm -o target/wasm32-unknown-unknown/release/demo_opt.wasm"
            ]
        }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        }
    },
    "version": 1
}
