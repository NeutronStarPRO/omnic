{
    "canisters": {
        "omnic": {
			  "candid": "src/omnic.did",
			  "package": "omnic-core",
			  "type": "rust",
			  "wasm": "target/wasm32-unknown-unknown/release/omnic.wasm",
			  "build": [
				"cargo build --target wasm32-unknown-unknown --release",
				"ic-cdk-optimizer target/wasm32-unknown-unknown/release/omnic.wasm -o target/wasm32-unknown-unknown/release/omnic_opt.wasm"
			  ]
        }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        }
    },
    "version": 1
}
